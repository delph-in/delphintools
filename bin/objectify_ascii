#!/bin/bash

objectify_ascii () {
	awk '
	BEGIN {
		FS=OFS="\n"
		RS=ORS=""
	}
	{
                if (match($1, /^(\[[0-9]+\] +)(.+)(;;[^;]+)$/, ids)) {
                        num = ids[1]
                        src = ids[2]
                        tid = ids[3]
		        tgt = num $2 tid
                }
                else {
                        src = $1
                        tgt = $2
                }

		print tgt, src, "\n"
	}'
}

objectify_ascii
